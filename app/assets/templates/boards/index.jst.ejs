<h1>Boards</h1>

<div class="boards_container"></div>

<% boards.forEach( function(board) { %>
  <div class="board_display_card">
    <a href="#/boards/<%= board.escape("id") %>">
      <%= board.escape("title") %>
    </a>
    <button 
      class="btn btn-default delete_board" 
      data-id="<%= board.id %>">Delete Board</button>
  </div>
<% }) %>

<div class="board_display_card">
  <a href="#/boards/new">Create a New Board</a>
</div>